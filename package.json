{
    "name": "remcostoeten-all-in-one-dashboard",
    "version": "0.1.0",
    "scripts": {
        "pushsqlite": "bunx drizzle-kit push:sqlite",
        "sqlite": "drizzle-kit push:sqlite",
        "dev": "next dev --turbo",
        "dev:next": "next dev",
        "build": "next build",
        "cleanup-imports": "eslint --fix --ext .ts,.tsx .",
        "start": "next start",
        "build-stats": "cross-env ANALYZE=true npm run build",
        "clean": "rimraf .next .swc out coverage",
        "lint": "next lint",
        "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
        "format": "next lint --fix && prettier '**/*.{json,yaml}' --write --ignore-path .gitignore",
        "typecheck": "tsc --noEmit --pretty",
        "commit": "git add * ; cz",
        "push": "git add * ; cz ;git push",
        "db:generate": "drizzle-kit generate",
        "build-full": "pnpm run cleanup-imports ; pnpm format:write ; mv .eslintrc.json oldeslint.json ; pnpm build ; mv oldeslint.json .eslintrc.json",
        "db:migrate": "drizzle-kit migrate",
        "db:studio": "drizzle-kit studio"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cleaner-commitizen-adapter"
        }
    },
